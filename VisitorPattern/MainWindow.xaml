<Window x:Class="VisitorPattern.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisitorPattern"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <Grid>
        <Grid x:Name="ElementGrid" HorizontalAlignment="Left" Height="634" Margin="10,93,0,0" VerticalAlignment="Top" Width="996" Background="#FF9C9C9C"/>
        <Button x:Name="redbutton" Content="Visit With Red Brush" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Width="453" Height="55" Background="#FFE8AFAF" Click="redbutton_Click"/>
        <Button x:Name="bluebutton" Content="Visit With Blue Brush" HorizontalAlignment="Left" Margin="525,21,0,0" VerticalAlignment="Top" Width="481" Height="55" Background="#FF6AA6EE" Click="bluebutton_Click"/>

    </Grid>
</Window>
